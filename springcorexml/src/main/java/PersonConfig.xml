<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="com.jspiders.springcorexml.bean.PersonBean" name="person1">
		<property name="id" value="1" />
		<property name="name" value="Ram" />
		<property name="email" value="Ram@gamil.com" />
		<property name="contact" value="9876543210" />
		<property name="aadhar" ref="aadhar1" />
	</bean>
	
	<bean class="com.jspiders.springcorexml.bean.AadharBean" name="aadhar1">
		<property name="id" value="1" />
		<property name="aadhar_no" value="778899445566" />
		<property name="date_of_issue" value="15-Aug-2005" />
	</bean>

</beans>