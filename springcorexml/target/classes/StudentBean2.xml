<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="com.jspiders.springcorexml.bean.StudentBean2" name="student1">
		<property name="id" value="1" />
		<property name="name" value="Ram" />
		<property name="email" value="ram@gmail.com" />
		<property name="address" value="Pune" />
		<property name="subjects">
			<list>
				<bean class="com.jspiders.springcorexml.bean.SubjectBean" name="subject1">
					<property name="id" value="1" />
					<property name="name" value="Core Java" />
					<property name="duration" value="3" />
				</bean>
				<bean class="com.jspiders.springcorexml.bean.SubjectBean" name="subject2">
					<property name="id" value="2" />
					<property name="name" value="Advance Java" />
					<property name="duration" value="2" />
				</bean>
			</list>
		</property>
	</bean>
	
	<bean class="com.jspiders.springcorexml.bean.StudentBean2" name="student2">
		<property name="id" value="2" />
		<property name="name" value="Sita" />
		<property name="email" value="sita@gmail.com" />
		<property name="address" value="Mumbai" />
		<property name="subjects">
			<list>
				<bean class="com.jspiders.springcorexml.bean.SubjectBean" name="subject1">
					<property name="id" value="1" />
					<property name="name" value="Core Java" />
					<property name="duration" value="3" />
				</bean>
				<bean class="com.jspiders.springcorexml.bean.SubjectBean" name="subject2">
					<property name="id" value="2" />
					<property name="name" value="Advance Java" />
					<property name="duration" value="2" />
				</bean>
			</list>
		</property>
	</bean>
</beans>